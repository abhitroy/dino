    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.hitomi.cmlibrary.CircleMenu
            android:id="@+id/circle_menu_timetable"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </RelativeLayout>

</RelativeLayout>
